<div>
  <h1>Mon top 5</h1>



  <%= turbo_frame_tag "mycinqs" do %>
    <%= render @mycinqs %>
  <% end %>

  <% (5 - @mycinqs.count).times do %>
    <div>
      <%= link_to new_year_mycinq_path(@year), remote: true, data: { turbo_frame: "modal" } do %>
        <%= image_tag "add_miss.svg" %>
      <% end %>
    </div>
  <% end %>
</div>

<%= link_to "Valider", mycinqs_others_year_path(@year) %>
<%= render "shared/bottombar" %>
