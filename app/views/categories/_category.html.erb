
  <% if category.critere == "bombe" && category.miss.year == @year%>
    <%= turbo_frame_tag "category_#{category.id}" do %>
    <ul>
      <%= category.miss.region %> 🔥
      <%= image_tag "#{category.miss.photo}.jpg"%>
      <%= link_to "voir", edit_year_miss_category_path(@year, category.miss, category), remote: true, data: { turbo_frame: "modal" } %>
    </ul>
    <% end %>
