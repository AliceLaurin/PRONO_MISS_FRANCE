<% @my_hot_misses_sorted.each do |category| %>
  <% if category.miss.year == @year  %>
  <ul>
    <%= category.miss.region %>
    <%= image_tag "#{category.miss.photo}.jpg"%>
    <%= link_to "voir", year_miss_path(@year,category.miss) %>
  </ul>
  <% end %>
<% end %>
